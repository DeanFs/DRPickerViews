  "platforms": {
    "ios": "9.0"
  },
  "resources": [
    "DRPickerViews/Assets/*",
    "DRPickerViews/Classes/**/*.xib"
  ],
  "subspecs": [
    {
      "name": "DatePickers",
      "source_files": "DRPickerViews/Classes/DatePickers/*.{h,m}",
      "subspecs": [
        {
          "name": "SubViews",
          "source_files": "DRPickerViews/Classes/DatePickers/SubViews/*.{h,m}"
        },
        {
          "name": "Classes",
          "source_files": "DRPickerViews/Classes/DatePickers/Classes/*.{h,m}",
          "subspecs": [
            {
              "name": "DRYMDWithLunarPicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRYMDWithLunarPicker/*.{h,m}",
              "subspecs": [
                {
                  "name": "DataSource",
                  "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRYMDWithLunarPicker/DataSource/*.{h,m}"
                }
              ]
            },
            {
              "name": "DRTimeConsumingPicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRTimeConsumingPicker/*.{h,m}"
            },
            {
              "name": "DRYMDPicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRYMDPicker/*.{h,m}"
            },
            {
              "name": "DRYMDWithTodayPicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRYMDWithTodayPicker/*.{h,m}"
            },
            {
              "name": "DRYearMonthPicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRYearMonthPicker/*.{h,m}"
            },
            {
              "name": "DRMDWTPicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRMDWTPicker/*.{h,m}"
            },
            {
              "name": "DRHourMinutePicker",
              "source_files": "DRPickerViews/Classes/DatePickers/Classes/DRHourMinutePicker/*.{h,m}"
            }
          ]
        }
      ]
    }
  ]
}
